
<h1>Attractions</h1>
<a href="/attractions/new">Add an Attraction</a>
<% @attractions.each do |attraction| %>
    <h2><a href="/attractions/<%= attraction.id %>"><%= attraction.name %></a></h2>
    <img src="<%= attraction.photo %>" alt ="<% attraction %>">
    <% if attraction.user && session[:user_id] == attraction.user.id %>
         <a href="/attractions/<%= attraction.id %>/edit">Edit</a>
    <% end %>
<% end %>